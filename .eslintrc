{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": 2,
    "no-var": 2,
    "prefer-arrow-callback": 2,
    "arrow-body-style": 2,
    "react/no-unused-prop-types": 0,
    "import/no-unresolved": 2,
    "import/newline-after-import": "error"
  },
  "globals": {
    "window": true,
    "navigator": true
  }
}
